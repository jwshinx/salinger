= form_for @address do |f|
 - if @address.errors.any?
  = render :partial => 'shared/error_explanation', :object => @address , :as => :object


 .field
  = f.label :street
  = f.text_field :street
 .field
  = f.label :suite
  = f.text_field :suite
 .field
  = f.label :city
  = f.text_field :city
 .field
  = f.label :state
  = f.text_field :state
 .field
  = f.label :zip
  = f.text_field :zip
 .actions
  = f.submit 'Save'
