= form_for @address, :html => {:id => 'address_form'} do |f|
 - if @address.errors.any?
  = render :partial => 'shared/error_explanation', :object => @address , :as => :object


 .field
  .width_medium
   = f.label :street
  = f.text_field :street, :class => 'required'
  #street_note{:style=>'display:none;'} xxxxx
 .field
  .width_medium
   = f.label :suite
  = f.text_field :suite
 .field
  .width_medium
   = f.label :city
  = f.text_field :city
 .field
  .width_medium
   = f.label :state
  = f.text_field :state
 .field
  .width_medium
   = f.label :zip
  = f.text_field :zip
 .actions
  = f.submit 'Save', :class => 'btn btn-primary'
